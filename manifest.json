{
  "manifest_version": 3,
  "name": "Auto Star",
  "version": "1.0",
  "description": "Automatically selects all 5 stars for teacher feedback.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://mrei.icloudems.com/*"],
  "content_scripts": [
    {
      "matches": ["https://mrei.icloudems.com/corecampus/quiz/student_feedback.php*"],
      "js": ["script.js"]
    }
    
  ],
  "icons": {
    "16": "AutoStar16.png",
    "32": "AutoStar32.png",
    "48": "AutoStar.png",
    "128": "AutoStar.png"
  }
  
}
