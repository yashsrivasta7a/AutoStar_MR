{
  "manifest_version": 3,
  "name": "Auto Star",
  "version": "2.0",
  "description": "Automatically selects all 5 stars for teacher feedback.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://mrei.icloudems.com/*"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://mrei.icloudems.com/corecampus/quiz/student_feedback.php*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "logo/16.jpg",
    "32":"logo/48.jpg",
    "48": "logo/48.jpg",
    "128": "logo/128x128.png"
  }
}
